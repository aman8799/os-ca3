#include<iostream>
#include<iomanip>
#include<algorithm>
#include<queue>
using namespace std;
struct process
	{	
		int pid, priority, burst, burst2, at, start, end, wt, tat;
	};
int ttime=0, tot=0;
int p;
struct process pro[100];

struct process find (int pid)
{
	for(int i=0; i<p; i++)
	{
		if(pro[i].pid==pid)
		{
			return pro[i];
		}
	}
}


